{
	"manifest_version": 2,
	"name": "Prevent Google Redirects",
	"version": "0.1",
	"description": "Prevents Google from taking you to its redirect page when clicking search results",
	
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"webNavigation",
		"<all_urls>"
	],
	
	"background": {
		"scripts": ["prevent-google-redirects.js"]
	},
	
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content-script.js"]
		}
	
	]

}
